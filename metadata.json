{
  "name": "cpts-crond",
  "version": "0.4.0",
  "author": "CPTS (mkraai), SIMP Team",
  "summary": "A Puppet module for managing crond",
  "license": "Apache-2.0",
  "source": "https://github.com/mtkraai/puppet-crond",
  "project_page": "https://github.com/mtkraai/puppet-crond",
  "dependencies": [
    {
      "name": "simp/simplib",
      "version_requirement": ">= 3.1.0 < 5.0.0"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 6.4.0 < 8.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 6.6.0 < 8.0.0"
    }
  ],
  "simp": {
    "optional_dependencies": [
      {
        "name": "simp/pam",
        "version_requirement": ">= 6.0.0 < 7.0.0"
      }
    ]
  },
  "operatingsystem_support": [
    {
      "operatingsystem": "Amazon",
      "operatingsystemrelease": [
        "2"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.22.1 < 8.0.0"
    }
  ]
}
